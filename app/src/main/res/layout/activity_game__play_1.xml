                            android:layout_height="@dimen/wh_45"
                            android:layout_marginBottom="@dimen/info_margin_bottom"
                            android:layout_marginLeft="@dimen/list_all"
                            android:layout_marginTop="@dimen/wh_70"
                            android:layout_toRightOf="@+id/qus_img2"
                            android:background="@drawable/txt_qus_bg"
                            android:gravity="center"
                            android:padding="@dimen/txt_10"
                            android:src="@drawable/a"
                            android:textColor="#000"
                            android:textSize="@dimen/frame_liner_textsize"
                            android:textStyle="bold"
                            android:typeface="sans" />

                        <ImageView
                            android:id="@+id/qus_img4"
                            android:layout_width="@dimen/wh_45"
                            android:layout_height="@dimen/wh_45"
                            android:layout_marginBottom="@dimen/info_margin_bottom"
