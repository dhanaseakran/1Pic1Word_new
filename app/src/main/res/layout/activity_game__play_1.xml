                            android:elevation="@dimen/lrtbp_5">

                            <ImageView
                                android:id="@+id/dimen_back"
                                android:layout_width="@dimen/wh_240"
                                android:layout_height="@dimen/wh_180"
                                android:visibility="gone" />

                            <TextView
