<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_lay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sundar1">

    <RelativeLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <nithra.word.game.onepiconeword.extra.MyTextView
            android:id="@+id/appname"
            android:layout_width="match_parent"
            android:layout_height="@dimen/title_height1"
            android:layout_alignParentTop="true"
            android:layout_margin="@dimen/pract_pading"
            android:gravity="center"
            android:text="1 PIC 1 WORD"
            android:textAppearance="?android:textAppearanceLarge"
            android:textColor="#000"
            android:textStyle="bold"
            android:visibility="gone" />

        <RelativeLayout
            android:id="@+id/top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/settings_img"
                android:layout_width="@dimen/title_height1"
                android:layout_height="@dimen/title_height1"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:layout_margin="@dimen/pract_pading"
                android:background="@drawable/setting_icon" />

            <ImageView
                android:id="@+id/theme_img"
                android:layout_width="@dimen/title_height1"
                android:layout_height="@dimen/title_height1"
                android:layout_margin="@dimen/pract_pading"
                android:layout_toRightOf="@+id/settings_img"
                android:background="@drawable/theme_icon" />


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="@dimen/title_height1"
                android:layout_centerInParent="true"
                android:layout_margin="@dimen/pract_pading"
                android:gravity="center">

                <ImageView
                    android:id="@+id/play_coin_img"
                    android:layout_width="@dimen/title_height1"
                    android:layout_height="@dimen/title_height1"
                    android:layout_centerInParent="true"
                    android:background="@drawable/play_coin"
                    android:gravity="center"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/leader_board_img"
                    android:layout_width="@dimen/title_height1"
                    android:layout_height="@dimen/title_height1"
                    android:layout_centerInParent="true"
                    android:background="@drawable/leader_icon"
                    android:gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/coin_lay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/pract_pading"
                android:layout_weight="0.33"
                android:background="@drawable/boder_red"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/lrtbp_5"
                    android:background="@drawable/play_coin" />

                <nithra.word.game.onepiconeword.extra.MyTextView
                    android:id="@+id/coins_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/title_height1"
                    android:layout_marginLeft="@dimen/lrtbp_5"
                    android:layout_marginRight="@dimen/lrtbp_5"
                    android:gravity="center|left"
                    android:text="0"
                    android:textAppearance="?android:textAppearanceMedium"
                    android:textColor="#333149"
                    android:textStyle="bold" />
            </LinearLayout>


        </RelativeLayout>

        <LinearLayout
